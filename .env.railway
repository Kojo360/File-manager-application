# Environment variables for Railway deployment
# These will be automatically set by Railway, but you can customize them

# Django settings
DEBUG=False
SECRET_KEY=your-secret-key-will-be-generated-by-railway

# Database (Railway provides this automatically)
DATABASE_URL=postgresql://user:password@host:port/database

# Allowed hosts (Railway provides this automatically)
ALLOWED_HOST=your-app-name.railway.app

# Static files
STATIC_URL=/static/
MEDIA_URL=/media/

# Port (Railway sets this automatically)
PORT=8000
